.body-bg{
    background-color: #f8f8f8;
}
/*loading*/
.spinner-w{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #ffffff;
    z-index: 1000;
}
.spinner {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin:auto;
    width: 50px;
    height: 60px;
    text-align: center;
    font-size: 10px;
}

.spinner > div {
    background-color: #f3f3f3;
    height: 100%;
    width: 6px;
    display: inline-block;
    margin: 2px;

    -webkit-animation: stretchdelay 1.2s infinite ease-in-out;
    animation: stretchdelay 1.2s infinite ease-in-out;
}

.spinner .rect2 {
    -webkit-animation-delay: -1.1s;
    animation-delay: -1.1s;
}

.spinner .rect3 {
    -webkit-animation-delay: -1.0s;
    animation-delay: -1.0s;
}

.spinner .rect4 {
    -webkit-animation-delay: -0.9s;
    animation-delay: -0.9s;
}

.spinner .rect5 {
    -webkit-animation-delay: -0.8s;
    animation-delay: -0.8s;
}

@-webkit-keyframes stretchdelay {
    0%, 40%, 100% { -webkit-transform: scaleY(0.4) }
    20% { -webkit-transform: scaleY(1.0) }
}

@keyframes stretchdelay {
    0%, 40%, 100% {
        transform: scaleY(0.4);
        -webkit-transform: scaleY(0.4);
    }  20% {
           transform: scaleY(1.0);
           -webkit-transform: scaleY(1.0);
       }
}
.product-about-w{
    background-color: #fff;
    padding: 10px 15px;
}
.nav-list{
    border-bottom: #e5e4e5 solid 1px;
    padding: 10px;
    line-height: 25px;
}
.nav-list a{
    color: #333333;
    display: inline-block;
    width: 49%;
    text-align: center;
    vertical-align: middle;
    position: relative;
}
.nav-list a:first-child:after{
    content: '';
    position: absolute;
    top: 7px;
    bottom: 7px;
    right: 0;
    border-left:#666666 solid 1px ;
}
.nav-list a.active{
    color: #0a988b;
}
.i-h{
    display: inline-block;
    width: 3px;
    height: 17px;
    background-color: #0a988b;
    vertical-align: middle;
    margin-right: 8px;
}
.product-about-title{
    padding: 20px 0;
    color: #333333;
    font-size: 16px;
}
.i-w{
    display: inline-block;
    vertical-align: middle;
    width: 20%;
}
.product-about-stitle{
    line-height: 25px;
    margin-bottom: 10px;
}